{
  "kind": "collectionType",
  "collectionName": "assessments",
  "info": {
    "singularName": "assessment",
    "pluralName": "assessments",
    "displayName": "Assessment",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "exercise": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::exercise.exercise",
      "inversedBy": "assessments"
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "assessments"
    },
    "option": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "assessment-options.options"
      ],
      "required": true
    },
    "explaination": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::slug.slug",
      "inversedBy": "assessments"
    },
    "question": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "required": false,
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
