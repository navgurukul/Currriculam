{
  "kind": "collectionType",
  "collectionName": "slugs",
  "info": {
    "singularName": "slug",
    "pluralName": "slugs",
    "displayName": "Slug"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "assessments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::assessment.assessment",
      "mappedBy": "slug"
    },
    "exercises": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exercise.exercise",
      "mappedBy": "slug"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "exercise",
        "assessment"
      ],
      "default": "exercise",
      "required": true
    }
  }
}
