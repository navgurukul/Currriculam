{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "short_description": {
      "type": "string",
      "maxLength": 255
    },
    "logo": {
      "type": "string"
    },
    "lang_available": {
      "type": "json"
    },
    "exercise_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exercise.exercise",
      "mappedBy": "course_id"
    },
    "assesment_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::assesment.assesment",
      "mappedBy": "course_id"
    }
  }
}
