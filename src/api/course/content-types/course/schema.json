{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "short_description": {
      "type": "string"
    },
    "pathway": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::pathway.pathway",
      "inversedBy": "courses"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "lang_available": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "en",
        "hi-IN",
        "kn-IN",
        "te-IN",
        "or-I"
      ],
      "required": true
    },
    "modules": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::module.module",
      "inversedBy": "courses"
    },
    "exercises": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::exercise.exercise",
      "mappedBy": "course"
    },
    "assessments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::assessment.assessment",
      "mappedBy": "course"
    },
    "slugs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::slug.slug",
      "mappedBy": "course"
    },
    "android_logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "course_type": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "pre-quiz"
      ],
      "required": true
    },
    "prequizs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::prequiz.prequiz",
      "mappedBy": "course"
    }
  }
}
